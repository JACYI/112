void setup() { pinMode(9, OUTPUT); pinMode(6, OUTPUT); pinMode(5, OUTPUT); pinMode(7, OUTPUT); pinMode(12, OUTPUT); pinMode(11, OUTPUT);  //发红色光 pinMode(10, OUTPUT);  //发蓝色光 pinMode(3, OUTPUT);   //发绿色光 }
void loop() { digitalWrite(9,HIGH); digitalWrite(6,HIGH); digitalWrite(5,HIGH); digitalWrite(7,HIGH); digitalWrite(12,HIGH); digitalWrite(13,HIGH); digitalWrite(3,HIGH); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(800); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(500); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(800); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(500); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(800); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(500); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(800); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(500); /************************************************************/ digitalWrite(11,HIGH); digitalWrite(10,HIGH); digitalWrite(3,HIGH); delay(1000); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(1200); /*************************************************************/ digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(300); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(100); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(300);digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(200);
  digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(150); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(150); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(500);digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(300); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(100); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(300);digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(200);
  digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(150); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(150); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,HIGH); digitalWrite(10,LOW); digitalWrite(3,HIGH); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(50); digitalWrite(11,LOW); digitalWrite(10,HIGH); digitalWrite(3,LOW); delay(200); digitalWrite(11,LOW); digitalWrite(10,LOW); digitalWrite(3,LOW); delay(500); }
